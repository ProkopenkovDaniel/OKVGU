@model IEnumerable<OKVGU.Models.GroupTotal>
@{
    ViewBag.Title = "Группы";
}
<div id="modDialog" class="modal fade">
    <div id="dialogContent" class="modal-dialog"></div>
</div>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1>Группы</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-10">
            <table class="table">
                <tbody>
                    <tr>
                        <th>
                            Название
                        </th>
                        <th>
                            Специальность
                        </th>
                    </tr>
                    @foreach (OKVGU.Models.GroupTotal group in Model)
                    {
                        @Html.Partial("GroupTip", group);
                    }
                </tbody>
            </table>
        </div>
        <div class="col-lg-2 controlBtn">
            @Html.Partial("ControlBtn");
        </div>
    </div>
</div>
